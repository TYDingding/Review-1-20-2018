#include<iostream>
#include<cstring>
#include<cctype>
#include<cmath>;
using namespace std;

void main()
{
	int M[4][4] = { {11,12,13,14},{21,22,23,24},{31,31,33,34},{41,42,43,44} };

	for (int i = 0; i < 4; i++)
	{
		for (int j = 0; j < 4; j++)
		{
			cout << M[i][j] << " ";
		}
		cout << endl;
	}
	
	cout << endl;

	for (int i = 0; i < 4; i++)
	{
		for (int j = 0; j < 4; j++)
		{
			swap(M[i][j], M[j][i]);
			cout << M[i][j]<<" ";
		}
		cout << endl;
	}
}
